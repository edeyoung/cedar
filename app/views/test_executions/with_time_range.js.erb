$('#coverage').html("<%= escape_javascript(render partial: 'dashboard_coverage', locals: {tests: @tests_complete, validations: @validations}) %>");
$('#totals').html("<%= escape_javascript(render partial: 'dashboard_totals', locals: {test_executions_num: @test_executions.length, tests_passed_num: @tests_passed.length, tests_failed_num: @tests_failed.length, tests_incomplete_num: @tests_incomplete.length}) %>");
$('#panels').html("<%= escape_javascript(render partial: 'test_executions/dashboard_panel_tests', locals: {panel_title: 'PASSED', tests: @tests_passed}) %> <%= escape_javascript(render partial: 'test_executions/dashboard_panel_tests', locals: {panel_title: 'FAILED', tests: @tests_failed}) %> <%= escape_javascript(render partial: 'test_executions/dashboard_panel_tests', locals: {panel_title: 'INCOMPLETE', tests: @tests_incomplete}) %>");
